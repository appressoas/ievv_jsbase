{
  "destination": "built_docs",
  "excludes": [
    "__tests__.+$",
    "__mocks__.+$"
  ],
  "includes": [
    "\\.(js|es6|jsx)$"
  ],
  "index": "esdoc/index.md",
  "manual": {
    "tutorial": [
      "esdoc/manual/tutorial.md"
    ]
  },
  "plugins": [
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {
            "from": "^source/",
            "to": "lib/"
          },
          {
            "from": ".jsx",
            "to": ""
          },
          {
            "from": ".js",
            "to": ""
          }
        ]
      }
    }
  ],
  "source": "source",
  "title": "ievv_jsbase"
}
